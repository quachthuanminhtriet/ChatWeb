{
    "build": {
      "src": "dist/webchat-cloud",
      "use": "@vercel/static-build",
      "buildCommand": "npm run build"
    },
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ],
    "cleanUrls": true,
    "trailingSlash": false,
    "outputDirectory": "dist/webchat-cloud"
  }
  